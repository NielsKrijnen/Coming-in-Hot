generator client {
  provider   = "prisma-client"
  output     = "../generated/prisma"
  engineType = "client"
  runtime    = "bun"
}

datasource db {
  provider = "sqlite"
}

model Server {
  id           Int    @id @default(autoincrement())
  containerId  String
  name         String
  port         Int
  rconPort     Int
  rconPassword String
}
