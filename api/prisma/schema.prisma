generator client {
  provider      = "prisma-client"
  output        = "../generated/prisma"
  binaryTargets = ["debian-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Server {
  id           Int    @id @default(autoincrement())
  containerId  String
  name         String
  port         Int
  rconPort     Int
  rconPassword String
}
